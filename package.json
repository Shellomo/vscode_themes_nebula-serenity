{
  "name": "nebula-serenity",
  "displayName": "Nebula Serenity",
  "description": "A sophisticated VS Code theme designed for focus and productivity",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Themes"],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Nebula Serenity",
        "uiTheme": "vs-dark",
        "path": "./themes/nebula-serenity-color-theme.json"
      }
    ]
  },
  "publisher": "Shellomo",
  "icon": "media/nebula-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shellomo/vscode_themes_nebula-serenity"
  },
  "keywords": [
    "theme",
    "dark theme",
    "nebula",
    "focus",
    "productivity"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.x.x",
    "@typescript-eslint/eslint-plugin": "^5.x.x",
    "@typescript-eslint/parser": "^5.x.x",
    "eslint": "^8.x.x",
    "typescript": "^4.x.x"
  },
  "dependencies": {
    "@vscode/extension-telemetry": "^0.9.8"
  }
}